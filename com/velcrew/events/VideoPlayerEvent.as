/* AS3	Copyright 2009 __Velcrew__.*/package com.velcrew.events{	import flash.events.Event;		/**	 *	VideoPlayerEvent	 *	 *	@langversion ActionScript 3.0	 *	@playerversion Flash 9.0	 *	 *	@author Luis Teixeira	 *	@since  25.06.2009	 */	public class VideoPlayerEvent extends Event {				/**		* Dispatched when the video init		*/		public static const VIDEO_INIT	: String = "onInit";				/**	    * Dispatched when the video starts playing	    */		public static const VIDEO_STARTED		: String = "onStart";		/**	    * Dispatched when the video stops playing 	    */		public static const VIDEO_STOPED 		: String = "onStop";		/**	    * Dispatched when the video ended playing 	    */		public static const VIDEO_COMPLETED		: String = "onComplete";		/**    	* Dispatched when the video ended playing 		*/		public static const VIDEO_FULLSCREEN	: String = "fullScreen";		/**	    * Dispatched when the video ended playing 	    */		public static const VIDEO_NORMAL		: String = "normal";						public static const ON_METADATA			: String = "onMetadata";						public var file 						: String = "";		public var message 						: String = "";		public var dataObj 						: Object = null ;		/**		 *	@constructor		 */		public function VideoPlayerEvent( type:String,file:String="",message:String="",dataObj:Object = null, bubbles:Boolean=true, cancelable:Boolean=false ){			super(type, bubbles, cancelable);			this.file = file;			this.message = message;			this.dataObj = dataObj;				}				//--------------------------------------		//  GETTER/SETTERS		//--------------------------------------				//--------------------------------------		//  PUBLIC METHODS		//--------------------------------------		override public function clone() : Event {			return new VideoPlayerEvent(type, file, message, dataObj, bubbles, cancelable);		}				//--------------------------------------		//  EVENT HANDLERS		//--------------------------------------		//--------------------------------------		//  PRIVATE VARIABLES		//--------------------------------------				//--------------------------------------		//  PRIVATE & PROTECTED INSTANCE METHODS		//--------------------------------------			}	}